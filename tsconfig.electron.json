{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "dist-electron",
    "module": "commonjs",
    "target": "es6",
    "noEmit": false,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowImportingTsExtensions": false,
    "resolveJsonModule": true
  },
  "include": ["electron/**/*"]
}
