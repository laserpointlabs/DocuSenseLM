<?xml version="1.0" encoding="UTF-8"?>
<bpmn2:definitions xmlns:bpmn2="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:camunda="http://camunda.org/schema/1.0/bpmn" xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" xmlns:di="http://www.omg.org/spec/DD/20100524/DI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" id="_Rfl3gKIkED6wWsl0mBkpDA" targetNamespace="http://www.omg.org/bpmn20" exporter="Camunda Modeler" exporterVersion="5.41.0">
  <!-- Message Definition (must be at root level) -->
  <bpmn2:message id="Message_CustomerSigned" name="CustomerSignedMessage" />
  
  <bpmn2:collaboration id="_FB32A87F-1841-4A03-92CC-F6C5364B55B6" name="Default Collaboration">
    <bpmn2:participant id="_B6638EFD-7396-4F41-B059-05C341F183F6" name="Pool Participant" processRef="nda_review_approval" />
  </bpmn2:collaboration>
  <bpmn2:process id="nda_review_approval" name="NDA Review and Approval" isExecutable="true" camunda:historyTimeToLive="90">
    <!-- Start Event: NDA Created -->
    <bpmn2:startEvent id="StartEvent_1" name="NDA Created">
      <bpmn2:outgoing>Flow_1</bpmn2:outgoing>
    </bpmn2:startEvent>
    
    <!-- Sequence Flow: Start → LLM Review -->
    <bpmn2:sequenceFlow id="Flow_1" sourceRef="StartEvent_1" targetRef="Task_LLMReview" />
    
    <!-- Service Task: LLM Review (External Task) -->
    <bpmn2:serviceTask id="Task_LLMReview" name="LLM Review" camunda:type="external" camunda:topic="llm_review">
      <bpmn2:extensionElements>
        <camunda:inputOutput>
          <camunda:inputParameter name="nda_record_id">${nda_record_id}</camunda:inputParameter>
        </camunda:inputOutput>
      </bpmn2:extensionElements>
      <bpmn2:incoming>Flow_1</bpmn2:incoming>
      <bpmn2:outgoing>Flow_2</bpmn2:outgoing>
    </bpmn2:serviceTask>
    
    <!-- Sequence Flow: LLM Review → Gateway -->
    <bpmn2:sequenceFlow id="Flow_2" sourceRef="Task_LLMReview" targetRef="Gateway_LLMDecision" />
    
    <!-- Gateway: LLM Approved? -->
    <bpmn2:exclusiveGateway id="Gateway_LLMDecision" name="LLM Approved?" gatewayDirection="Diverging">
      <bpmn2:incoming>Flow_2</bpmn2:incoming>
      <bpmn2:outgoing>Flow_3</bpmn2:outgoing>
      <bpmn2:outgoing>Flow_4</bpmn2:outgoing>
    </bpmn2:exclusiveGateway>
    
    <!-- Sequence Flow: Gateway → Rejected End -->
    <bpmn2:sequenceFlow id="Flow_4" name="Rejected" sourceRef="Gateway_LLMDecision" targetRef="EndEvent_Rejected">
      <bpmn2:conditionExpression xsi:type="bpmn2:tFormalExpression">${llm_approved == false}</bpmn2:conditionExpression>
    </bpmn2:sequenceFlow>
    
    <!-- End Event: Rejected -->
    <bpmn2:endEvent id="EndEvent_Rejected" name="Rejected">
      <bpmn2:incoming>Flow_4</bpmn2:incoming>
    </bpmn2:endEvent>
    
    <!-- Sequence Flow: Gateway → Human Review -->
    <bpmn2:sequenceFlow id="Flow_3" name="Approved" sourceRef="Gateway_LLMDecision" targetRef="Task_HumanReview">
      <bpmn2:conditionExpression xsi:type="bpmn2:tFormalExpression">${llm_approved == true}</bpmn2:conditionExpression>
    </bpmn2:sequenceFlow>
    
    <!-- User Task: Human Review -->
    <bpmn2:userTask id="Task_HumanReview" name="Human Review" camunda:assignee="${reviewer_user_id}">
      <bpmn2:incoming>Flow_3</bpmn2:incoming>
      <bpmn2:outgoing>Flow_5</bpmn2:outgoing>
    </bpmn2:userTask>
    
    <!-- Sequence Flow: Human Review → Gateway -->
    <bpmn2:sequenceFlow id="Flow_5" sourceRef="Task_HumanReview" targetRef="Gateway_HumanDecision" />
    
    <!-- Gateway: Human Approved? -->
    <bpmn2:exclusiveGateway id="Gateway_HumanDecision" name="Human Approved?" gatewayDirection="Diverging">
      <bpmn2:incoming>Flow_5</bpmn2:incoming>
      <bpmn2:outgoing>Flow_6</bpmn2:outgoing>
      <bpmn2:outgoing>Flow_7</bpmn2:outgoing>
    </bpmn2:exclusiveGateway>
    
    <!-- Sequence Flow: Gateway → Rejected End -->
    <bpmn2:sequenceFlow id="Flow_7" name="Rejected" sourceRef="Gateway_HumanDecision" targetRef="EndEvent_RejectedHuman">
      <bpmn2:conditionExpression xsi:type="bpmn2:tFormalExpression">${human_approved == false}</bpmn2:conditionExpression>
    </bpmn2:sequenceFlow>
    
    <!-- End Event: Rejected by Human -->
    <bpmn2:endEvent id="EndEvent_RejectedHuman" name="Rejected by Human">
      <bpmn2:incoming>Flow_7</bpmn2:incoming>
    </bpmn2:endEvent>
    
    <!-- Sequence Flow: Gateway → Send to Customer -->
    <bpmn2:sequenceFlow id="Flow_6" name="Approved" sourceRef="Gateway_HumanDecision" targetRef="Task_SendToCustomer">
      <bpmn2:conditionExpression xsi:type="bpmn2:tFormalExpression">${human_approved == true}</bpmn2:conditionExpression>
    </bpmn2:sequenceFlow>
    
    <!-- Service Task: Send to Customer (External Task) -->
    <bpmn2:serviceTask id="Task_SendToCustomer" name="Send to Customer" camunda:type="external" camunda:topic="send_to_customer">
      <bpmn2:extensionElements>
        <camunda:inputOutput>
          <camunda:inputParameter name="nda_record_id">${nda_record_id}</camunda:inputParameter>
          <camunda:inputParameter name="customer_email">${customer_email}</camunda:inputParameter>
        </camunda:inputOutput>
      </bpmn2:extensionElements>
      <bpmn2:incoming>Flow_6</bpmn2:incoming>
      <bpmn2:outgoing>Flow_8</bpmn2:outgoing>
    </bpmn2:serviceTask>
    
    <!-- Sequence Flow: Send to Customer → Wait for Signature -->
    <bpmn2:sequenceFlow id="Flow_8" sourceRef="Task_SendToCustomer" targetRef="IntermediateCatchEvent_CustomerSigned" />
    
    <!-- Intermediate Catch Event: Wait for Customer Signature (Message Event) -->
    <bpmn2:intermediateCatchEvent id="IntermediateCatchEvent_CustomerSigned" name="Customer Signed">
      <bpmn2:incoming>Flow_8</bpmn2:incoming>
      <bpmn2:outgoing>Flow_9</bpmn2:outgoing>
      <bpmn2:messageEventDefinition messageRef="Message_CustomerSigned" />
    </bpmn2:intermediateCatchEvent>
    
    <!-- Sequence Flow: Customer Signed → LLM Review Signed -->
    <bpmn2:sequenceFlow id="Flow_9" sourceRef="IntermediateCatchEvent_CustomerSigned" targetRef="Task_LLMReviewSigned" />
    
    <!-- Service Task: LLM Review Signed Document (External Task) -->
    <bpmn2:serviceTask id="Task_LLMReviewSigned" name="LLM Review Signed Document" camunda:type="external" camunda:topic="llm_review_signed">
      <bpmn2:extensionElements>
        <camunda:inputOutput>
          <camunda:inputParameter name="nda_record_id">${nda_record_id}</camunda:inputParameter>
        </camunda:inputOutput>
      </bpmn2:extensionElements>
      <bpmn2:incoming>Flow_9</bpmn2:incoming>
      <bpmn2:outgoing>Flow_10</bpmn2:outgoing>
    </bpmn2:serviceTask>
    
    <!-- Sequence Flow: LLM Review Signed → Gateway -->
    <bpmn2:sequenceFlow id="Flow_10" sourceRef="Task_LLMReviewSigned" targetRef="Gateway_LLMSignedDecision" />
    
    <!-- Gateway: LLM Approved Signed? -->
    <bpmn2:exclusiveGateway id="Gateway_LLMSignedDecision" name="LLM Approved Signed?" gatewayDirection="Diverging">
      <bpmn2:incoming>Flow_10</bpmn2:incoming>
      <bpmn2:outgoing>Flow_11</bpmn2:outgoing>
      <bpmn2:outgoing>Flow_12</bpmn2:outgoing>
    </bpmn2:exclusiveGateway>
    
    <!-- Sequence Flow: Gateway → Rejected End -->
    <bpmn2:sequenceFlow id="Flow_12" name="Rejected" sourceRef="Gateway_LLMSignedDecision" targetRef="EndEvent_RejectedSigned">
      <bpmn2:conditionExpression xsi:type="bpmn2:tFormalExpression">${llm_signed_approved == false}</bpmn2:conditionExpression>
    </bpmn2:sequenceFlow>
    
    <!-- End Event: Rejected Signed -->
    <bpmn2:endEvent id="EndEvent_RejectedSigned" name="Rejected Signed">
      <bpmn2:incoming>Flow_12</bpmn2:incoming>
    </bpmn2:endEvent>
    
    <!-- Sequence Flow: Gateway → Internal Signature -->
    <bpmn2:sequenceFlow id="Flow_11" name="Approved" sourceRef="Gateway_LLMSignedDecision" targetRef="Task_InternalSignature">
      <bpmn2:conditionExpression xsi:type="bpmn2:tFormalExpression">${llm_signed_approved == true}</bpmn2:conditionExpression>
    </bpmn2:sequenceFlow>
    
    <!-- User Task: Internal Signature -->
    <bpmn2:userTask id="Task_InternalSignature" name="Internal Signature" camunda:assignee="${internal_signer_user_id}">
      <bpmn2:incoming>Flow_11</bpmn2:incoming>
      <bpmn2:outgoing>Flow_13</bpmn2:outgoing>
    </bpmn2:userTask>
    
    <!-- Sequence Flow: Internal Signature → Approve & Release -->
    <bpmn2:sequenceFlow id="Flow_13" sourceRef="Task_InternalSignature" targetRef="EndEvent_Approved" />
    
    <!-- End Event: Approved & Released -->
    <bpmn2:endEvent id="EndEvent_Approved" name="Approved &amp; Released">
      <bpmn2:incoming>Flow_13</bpmn2:incoming>
    </bpmn2:endEvent>
  </bpmn2:process>
  
