{
  "name": "docusenselm",
  "version": "1.0.0",
  "description": "Simple desktop app for managing and interrogating NDAs/Agreements",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "vite",
    "build:python": "rm -rf python/dist/server && python/venv/bin/python -m PyInstaller --clean --name server --distpath ./python/dist --workpath ./python/build --specpath ./python --onefile --additional-hooks-dir=python --hidden-import=key_value ./python/server.py",
    "build": "npm run build:python && tsc && vite build && npm run electron:build && electron-builder",
    "preview": "vite preview",
    "electron:build": "tsc -p tsconfig.electron.json",
    "electron:dev": "npm run electron:build && concurrently -k \"cross-env BROWSER=none npm run dev\" \"wait-on tcp:5173 && electron .\""
  },
  "dependencies": {
    "@tailwindcss/typography": "^0.5.19",
    "clsx": "^2.1.0",
    "electron-updater": "^6.6.2",
    "js-yaml": "^4.1.0",
    "lucide-react": "^0.344.0",
    "pdfjs-dist": "^5.4.449",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^10.1.0",
    "react-pdf": "^10.2.0",
    "remark-gfm": "^4.0.1",
    "tailwind-merge": "^2.2.1"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.18",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "dotenv": "^17.2.3",
    "electron": "^29.1.0",
    "electron-builder": "^24.13.3",
    "electron-is-dev": "^3.0.1",
    "postcss": "^8.4.35",
    "sharp": "^0.34.5",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "wait-on": "^7.2.0"
  }
}
